{
  "expo": {
    "name": "GGM Field",
    "slug": "ggm-field-app",
    "version": "3.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "uk.co.gardnersgm.field",
      "supportsTablet": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "uk.co.gardnersgm.field",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "plugins": [
      "expo-location",
      "expo-camera",
      "expo-image-picker",
      "expo-secure-store",
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#2E7D32",
          "sounds": []
        }
      ],
      "expo-asset",
      [
        "expo-av",
        {
          "microphonePermission": "GGM Field needs microphone access for voice notes."
        }
      ],
      "expo-font"
    ],
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 5000,
      "url": "https://u.expo.dev/d17fe848-6644-4d9e-8745-895ab41ba6d0"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "extra": {
      "eas": {
        "projectId": "d17fe848-6644-4d9e-8745-895ab41ba6d0"
      }
    },
    "owner": "chrisgardner"
  }
}
